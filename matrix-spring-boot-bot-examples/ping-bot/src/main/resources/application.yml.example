spring:
  main:
    web-application-type: NONE

org:
  neo4j:
    driver:
      uri: bolt://localhost:7687
      authentication:
        username: neo4j
        password: secret

logging:
  level:
    net.folivo.matrix: DEBUG

matrix:
  bot:
    serverName: example.org
  client:
    homeServer:
      hostname: matrix.example.org
      secure: true
    token: superSecretToken