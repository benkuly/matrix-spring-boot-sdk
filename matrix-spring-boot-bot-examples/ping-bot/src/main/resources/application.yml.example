spring:
  main:
    web-application-type: NONE

org:
  neo4j:
    driver:
      uri: bolt://localhost:7687
      authentication:
        username: neo4j
        password: secret

logging:
  level:
    net.folivo.matrix: DEBUG

matrix:
  bot:
    serverName: example.org
    username: ping
    migration:
      url: jdbc:h2:mem:testdb
    database:
      url: r2dbc:h2:mem:///testdb
  client:
    homeServer:
      hostname: matrix.example.org
      secure: true
    token: superSecretToken